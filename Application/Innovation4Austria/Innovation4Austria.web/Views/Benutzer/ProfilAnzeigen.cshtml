@model Innovation4Austria.web.Models.ProfilAnzeigeModel

@{
    ViewBag.Title = "Profilansicht";
}

@Html.ValidationSummary(true, "", new { @class = "text-danger" })
<div class="container ProfilAnzeigen">
    <div class="row">
        <div class="col-md-12">
            <h1 style="font-size:4em">Profil verwalten</h1>
        </div>
    </div>
    <div class="row" id="profilAnsicht">
        <div class="col-md-4">
            @Html.Partial("_DatenAnzeigen", Model.derMitarbeiter)
        </div>
        <div class="col-md-offset-3 col-md-5">
            @Html.Partial("_PasswortVerwaltung", Model.anderesPasswort)
        </div>

    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
