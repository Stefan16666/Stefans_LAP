@model List<Innovation4Austria.web.Models.BenutzerModel>
@{
    ViewBag.Title = "MitarbeiterBearbeiten";
}

@if (Model.Count==0)
{
    <h1 class="keineMitarbeitergefunden">noch keine Mitarbeiter vorhanden</h1>
}

<div class="form-horizontal">
    <div class="container" id="firmenauflistung">


        @foreach (var item in Model)
        {
            @Html.Partial("_MitarbeiterBearbeiten", item)
        }

    </div>

</div>

