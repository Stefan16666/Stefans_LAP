@model Innovation4Austria.web.Models.FirmaAnlegenModel
@{
    ViewBag.Title = "FirmaAnlegen";
}


<div class="container" style="margin-top:20em">
    <div class="row">
        <div class="col-md-12" style="text-align:center">
            <div class="FontSize">
                @Html.LabelFor(x => x.Bezeichnung)
                @Html.TextBoxFor(x => x.Bezeichnung)
            </div>
            <div class="FontSize">
                @Html.LabelFor(x => x.Strasse)
                @Html.TextBoxFor(x => x.Strasse)
            </div>
            <div class="FontSize">
                @Html.LabelFor(x => x.Nummer)
                @Html.TextBoxFor(x => x.Nummer)
            </div>
            <div class="FontSize">
                @Html.LabelFor(x => x.Strasse)
                @Html.TextBoxFor(x => x.Strasse)
            </div>
            <div class="FontSize">
                @Html.LabelFor(x => x.Strasse)
                @Html.TextBoxFor(x => x.Strasse)
            </div>
            <div class="FontSize">
                @Html.LabelFor(x => x.Strasse)
                @Html.TextBoxFor(x => x.Strasse)
            </div>
        </div>
    </div>
</div>
